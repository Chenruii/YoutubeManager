{% extends 'base.html.twig' %}

{% block title %}Hello UserController!{% endblock %}

{% block body %}

    <div class="content">

        <ul>
            {{ form_start(form) }}
            {{ form_widget(form) }}
            {{ form_end(form) }}
        </ul>

        {% for user in users %}
            <h1>{{ user.firstname }}</h1>
            {% for video in user.videos %}
                <p>{{ video.title }}</p>
            {% endfor %}
        {% endfor %}
    </div>
{% endblock %}
