{% extends 'base.html.twig' %}

{% block title %}Hello VideoController!{% endblock %}

{% block body %}
    <div class="content">
        <h1>Video</h1>

        <ul>
            {% for videos in video %}
                <a href="{{ path('details_videos', { 'id': video.id }) }}">{{ video.title }}</a>
                <li>{{ video.url }}</li>
                <br>

            {% endfor %}
        </ul>


    </div>

{% endblock %}
